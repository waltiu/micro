{
  "name": "micro",
  "private": true,
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel  start:*",
    "instal": "npm-run-all --serial instal:*",
    "start:host-umi": "cd packages/HostUmi && npm start",
    "start:host-webpack":"cd packages/HostWebpack && npm start",
    "start:remote-umi": "cd packages/RemoteUmi && npm start",
    "start:remote-webpack": "cd packages/RemoteWebpack && npm start",
    "instal:host-umi": "cd packages/HostUmi && npm install",
    "instal:host-webpack":"cd packages/HostWebpack && npm install",
    "instal:remote-umi": "cd packages/RemoteUmi && npm install",
    "instal:remote-webpack": "cd packages/RemoteWebpack && npm install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/waltiu/micro.git"
  },
  "author": "",
